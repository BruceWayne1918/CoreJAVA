class Even extends Thread
{
	Even()
	{
		start();
	}
	public void run()
	{
		try
		{
			for(int i=0;i<10;i+=2)
			{
				System.out.println("Even = "+i);
				Thread.sleep(2500);
			}
		}
	}
	
	
}
public class Day21_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
