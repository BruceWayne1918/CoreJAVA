class Even extends Thread
{
	Even()
	{
		start();
	}
	public void run()
	{
		try
		{
			for(int i=0;i<10;i+=2)
			{
				System.out.println("Even = "+i);
				Thread.sleep(2500);
			}
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
	}
}


class Odd extends Thread
{
	Odd()
	{
		start();
	}
	public void run()
	{
		try
		{
			for(int i=1;i<10;i+=2)
			{
				System.out.println("Odd = "+i);
				Thread.sleep(2500);
			}
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
	}
}

