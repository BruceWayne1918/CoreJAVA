class Count implements Runnable
{
	public void run()
	{
		try
		{
			for(int i=0;i<5;i++)
			{
				System.out.println("Count = "+i);
				Thread.sleep(2000);
			}
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
	}
	
}
public class Day21_5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
